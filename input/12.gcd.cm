
int x, y;

int gcd() {
      int t;
         if (y==0) {
            return x;
	 }
         else {
	      t = x;
	      x = y;
	      y = t - y * (t / y);
	      return gcd();
	 }
}

int main() {
         write('enter x:');
         x = 3;
	 write('enter y:');
         y = 3;
         while ((x != 0) || (y != 0)) {
	       write (gcd());
	       write('enter x:');
               read (x);
	       write('enter y:');
               read (y);
         }
}
